{
    "request":{
        "method": "GET",
        "urlPathPattern": "/vas/v1/eligibility/(.*)",
        "queryParameters" : {
          "operations" : {
            "equalTo" : "ADESAO"
          }
        }
    },
    "response" : {
        "status": 200,
        "jsonBody": [
			{
				"eligible": true,
				"operation": "ADESAO"
			}
		]
    }
}

